{-------------------------------------------------------------------------------
  Procedure: GetDefaultLangByLocale
  Author:    jaro.benes
  DateTime:  2011.03.02
  Arguments: None
  Result:    TRxLang
  Purpose:   Local function for evaluate language by system locale
             (by default is English)
-------------------------------------------------------------------------------}

function GetDefaultLangByLocale: TRxLang;
var
  L: Integer;
begin
  L := SysUtils.SysLocale.PriLangID;
  case L of
    LANG_NEUTRAL: Result := rlEng;
    LANG_BULGARIAN: Result := rlBul;
    LANG_CHINESE:
      begin
        case SysUtils.SysLocale.SubLangID of
          SUBLANG_CHINESE_TRADITIONAL: Result := rlCht;
          SUBLANG_CHINESE_SIMPLIFIED: Result := rlChi;
        else
          Result := rlChi;
        end;
      end;
    LANG_CZECH: Result := rlCze;
    LANG_DANISH: Result := rlDan;
    LANG_DUTCH: Result := rlDut;
    LANG_ENGLISH: Result := rlEng;
    LANG_FRENCH: Result := rlFra;
    //not default lang
    //cannot be evaluate by locale, must be set up manually
    //LANG_GALICIAN: Result := rlGal;
    LANG_GERMAN: Result := rlGer;
    LANG_GREEK: Result := rlGre;
    LANG_HUNGARIAN: Result := rlHun;
    LANG_ITALIAN: Result := rlIta;
    LANG_KOREAN: Result := rlKor;
    LANG_POLISH: Result := rlPol;
    LANG_PORTUGUESE: Result := rlPor;
    LANG_ROMANIAN: Result := rlRom;
    LANG_RUSSIAN: Result := rlRus;
    LANG_SLOVENIAN: Result := rlSlo;
    LANG_SPANISH:
      begin
        case SysUtils.SysLocale.SubLangID of
          SUBLANG_SPANISH_MEXICAN: Result := rlMex;
        else
          Result := rlSpa;
        end;
      end;
    LANG_SERBIAN: Result := rlSrb;
    LANG_SLOVAK: Result := rlSvk;
    LANG_SWEDISH: Result := rlSwe;
    LANG_TURKISH: Result := rlTur;
    LANG_UKRAINIAN: Result := rlUkr;
  end;
end;

initialization
  RxDefaultLang := GetDefaultLangByLocale;
end.